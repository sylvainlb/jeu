<h1>Listing zones</h1>

<table>
  <tr>
    <th>Region id</th>
    <th>Buildingtype id</th>
    <th>Coordx</th>
    <th>Coordy</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @zones.each do |zone| %>
  <tr>
    <td><%= zone.region %></td>
    <td><%= zone.buildingType %></td>
    <td><%= zone.coordX %></td>
    <td><%= zone.coordY %></td>
    <td><%= link_to 'Show', zone %></td>
    <td><%= link_to 'Edit', edit_zone_path(zone) %></td>
    <td><%= link_to 'Destroy', zone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Zone', new_zone_path %>
